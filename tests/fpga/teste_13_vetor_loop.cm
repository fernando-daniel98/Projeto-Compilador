/* Teste 13: Manipulação de vetores com loop */
void main(void){
    int numeros[3];
    int i;
    int soma;
    
    i = 0;
    soma = 0;
    
    numeros[0] = 10;
    numeros[1] = 20;
    numeros[2] = 30;
    
    while (i < 3) {
        soma = soma + numeros[i];
        i = i + 1;
    }
    
    output(soma);
}
