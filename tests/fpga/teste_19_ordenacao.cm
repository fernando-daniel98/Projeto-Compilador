/* Teste 19: Algoritmo de ordenação simples (bubble sort) */
void ordenar(int arr[], int tamanho) {
    int i;
    int j;
    int temp;
    
    i = 0;
    while (i < tamanho - 1) {
        j = 0;
        while (j < tamanho - 1 - i) {
            if (arr[j] > arr[j + 1]) {
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
            j = j + 1;
        }
        i = i + 1;
    }
}

void main(void){
    int numeros[4];
    
    numeros[0] = 40;
    numeros[1] = 10;
    numeros[2] = 30;
    numeros[3] = 20;
    
    ordenar(numeros, 4);
}
