/* Um programa que implementa uma calculadora. */

void main(void) {
    int sinal;
    int a;
    int b;
    int ans;

    sinal = input();
    a = input();
    b = input();

    if (sinal == 1){
        ans = a + b;
    }else if (sinal == 2){
        ans = a - b;
    }else if (sinal == 3){
        ans = a * b;
    }else{
        ans = a / b;
    }

    output(ans);
}