/* Um programa para calcular o fibonacci de n. */

int fibonacci(int n)
{
    int a; int b; int c; int i;
    a = 0;
    b = 1;
    i = 2;
    c = 0;
    if (i == 0) {
        return a;
    } else {
        while (i <= n) {
            c = a + b;
            a = b;
            b = c;
            i = i + 1;
        }
    }
    return b;
}

void main(void)
{	int n;
    n = input();
    output(fibonacci(n));
}