void main(void){
    int base;
    int exp;
    int res;
    int teste;
    int ctrl;

    res = 1;
    ctrl = 0;
    base = input();
    exp = input();

    while (ctrl == 0) {
        res = res * base;
        exp = exp - 1;
        teste = input();

        if (exp == 0){
            teste = input();
            ctrl = 1;
        }
    }

    output(res);
    teste = input();
}